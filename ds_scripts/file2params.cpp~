#include <iostream>


void main(int argc, char* argv[])
{
  std::ifstream in;
  std::string line;
  std::stringstream sl;
  std::string param;
  std::string s_value;
  double d_value;


  if (argc == 1)
    {
      in.open(argv[1]);
    }						
  else
    {
      in = cin;
    }

  int l = 1;
  cout << "{";
  while (!is.eof())
    {
      if ( l > 1)
	{
	  cout << ",";
	}
      is.getline(line);
      sl = stringstream(line);
      sl >> param;
      sl >> d_value;
      if (!sl)
	{
	  sl >> s_value;
	  if (length(s_value) == 0)
	    {
	      cerr << "ERROR parsing line " << l<< endl;
	      exit (-1);
	    }
	  cout << "\"" << param << "\":\"" << svalue << "\"";
	}
      else
	{
	  cout << "\"" << param<< "\":" << d_value;
	}
      l++;
    }

  cout << "}" << endl;
}

